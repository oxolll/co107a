/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b,b=sel,out=aandsel);
    Not(in=sel,out=notsel);
    And(a=notsel,b=a,out=notselanda);
    Or(a=aandsel,b=notselanda,out=out);
}
